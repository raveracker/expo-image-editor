{
  "expo": {
    "name": "example",
    "slug": "example",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "android": {
      "package": "com.thomascoldwell.expoimageeditor"
    }
  }
}
